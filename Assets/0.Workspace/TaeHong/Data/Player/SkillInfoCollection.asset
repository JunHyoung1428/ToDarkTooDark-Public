    attackName: "\uD55C \uBC88 \uBCA0\uAE30"
    skillName: "\uC0BC\uC5F0\uCC38"
    dashName: "\uB3C4\uC57D \uACF5\uACA9"
    ultimateName: "\uCD94\uC5B5"
    passiveName: "\uD54F\uC904"
    attackDescription: "\uAC15\uD558\uAC8C \uAC80\uC744 \uD718\uB458\uB7EC \uBCA0\uC5B4\uB0C5\uB2C8\uB2E4.
      \uB290\uB9AC\uC9C0\uB9CC \uAC15\uB825\uD569\uB2C8\uB2E4."
    skillDescription: "\uC2A4\uD0AC \uC785\uB825 \uC2DC \uCD1D 3\uBC88\uAE4C\uC9C0
      \uAC80\uC744 \uD718\uB458\uB7EC \uAC15\uD558\uAC8C \uACF5\uACA9\uD569\uB2C8\uB2E4.
      \uB04A\uC5B4\uC11C \uC0AC\uC6A9\uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4."
    dashDescription: "\uCC38\uC2A4\uD0AC \uD0A4\uB97C \uB204\uB974\uB294\uB3D9\uC548
      \uCD5C\uB300 1\uCD08\uAE4C\uC9C0 \uCC28\uC9D5\uD55C \uB4A4 \uB9C8\uC6B0\uC2A4
      \uCEE4\uC11C\uAC00 \uC788\uB294 \uBC29\uD5A5\uC73C\uB85C \uB3C4\uC57D\uD569\uB2C8\uB2E4.
      \uCC29\uC9C0 \uC9C0\uC810\uC5D0 \uC788\uB294 \uC801\uC740 1\uCD08\uAC04 \uAE30\uC808\uD569\uB2C8\uB2E4.
      \uB3C4\uC57D \uACBD\uB85C\uC5D0 \uC801\uC774 \uC788\uC744 \uACBD\uC6B0 \uC57D\uD55C
      \uB370\uBBF8\uC9C0\uB97C \uC785\uD788\uBA70 \uCC29\uC9C0 \uC9C0\uC810\uAE4C\uC9C0
      \uB04C\uACE0\uAC11\uB2C8\uB2E4."
    ultimateDescription: "\uC790\uC138\uB97C \uB2E4\uC7A1\uC740 \uB4A4 \uB3CC\uC9C4\uD558\uC5EC
      \uAC80\uC744 \uAC15\uB825\uD558\uAC8C \uD718\uB458\uB7EC \uC801\uC744 \uBA40\uB9AC
      \uD295\uACA8\uB0C5\uB2C8\uB2E4. \uB9DE\uC740 \uC801\uC740 \uC5D1\uC2A4\uCE7C\uB9AC\uBC84\uB97C
      \uB4E4\uACE0 \uC788\uC744 \uACBD\uC6B0 \uB5A8\uC5B4\uD2B8\uB9BD\uB2C8\uB2E4.
      \uC801\uC740 \uBCBD\uC774 \uC788\uB294 \uACF3\uAE4C\uC9C0 \uB0A0\uC544\uAC00\uBA70
      \uBCBD\uC5D0 \uB9DE\uC744 \uACBD\uC6B0 1\uCD08\uAC04 \uAE30\uC808\uD569\uB2C8\uB2E4."
    passiveDescription: "\uAC15\uB825\uD55C \uCC99 \uAC00\uBB38\uC758 \uD54F\uC904\uC774
      \uD750\uB985\uB2C8\uB2E4. \uCD5C\uC885 \uC2A4\uD0EF\uC774 \uC99D\uAC00\uB418\uACE0
      \uC0C1\uD0DC\uC774\uC0C1\uC758 \uC601\uD5A5\uC744 \uB35C \uBC1B\uAC8C \uB429\uB2C8\uB2E4."
